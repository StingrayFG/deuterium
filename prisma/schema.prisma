generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model file {
  uuid        String    @id
  name        String
  hashSum     String?
  uploadIP    String?
  uploadDate  DateTime? @db.Timestamptz(3)  @default(dbgenerated("NOW()")) 
}

model panelToken {
  value          String    @id
  expirationDate DateTime? @db.Timestamptz(3)  @default(dbgenerated("NOW() + interval '300 seconds'")) 
}

model panelUser {
  login       String    @id
  password    String 
}
